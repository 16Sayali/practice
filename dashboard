style.css
.sidebar {
    margin: 0;
    padding: 0;
    width: 200px;
    background-color: #f1f1f1;
    position: fixed;
    height: 100%;
    overflow: auto;
  }
  /* Sidebar links */
  .sidebar a {
    display: block;
    color: black;
    padding: 16px;
    text-decoration: none;
  }
  /* Active/current link */
  .sidebar a.active {
    background-color: #046daa;
    color: white;
  }
/* Links on mouse-over */
.sidebar a:hover:not(.active) {
    background-color: #555;
    color: white;
  }
  
  /* Page content. The value of the margin-left property should match the value of the sidebar's width property */
  div.content {
    margin-left: 200px;
    padding: 1px 16px;
    height: 1000px;
  }
  section{
    display: flex;
    margin-left: 10px;
  }
  .w3-container{
    align-content: center;
  }
  div.content {
    margin-left: 200px;
    padding: 1px 14px;
    height: 1000px;
  }
  div.box{
    margin-left: 100px;
    padding: 2px 10px;
    
  } 
  .card { 
    box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);
    margin-left: 200px;
    padding: 2px 10px;
    width: 40%;
  }
  .card:hover { 
    box-shadow: 0 8px 16px 0 rgba(0,0,0,0.2);
  }
  
  .container1 {
    padding: 2px 16px;
  }
  .card-group{
    margin-left: 100;
    padding: 10px;
  } 
  .report-container {    
    display: flex;        
    min-height: 300px;
    max-width: 1400px;
    margin: 70px auto 0px auto;
    background-color: #ffffff;
    border-radius: 30px;
    box-shadow: 3px 3px 10px rgb(188, 188, 188);
    padding: 0px 20px 20px 20px;
    text-size-adjust: 10;
  }
  * {
    box-sizing: border-box;
  }
  .column {
    float: left;
    width: 60.33%;
    padding: 5px;
  }
  /* Clearfix (clear floats) */
  .row::after {
    content: "";
    clear: both;
    display: table;
  }
  #interface{
    display: flex;
    margin-left: 600px;
    width: 100px;
